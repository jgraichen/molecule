{
  "name": "molecule",
  "version": "0.1.0",
  "description": "Reactive UI molecules to build higher level components",
  "main": "lib/index.coffee",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgraichen/molecule.git"
  },
  "keywords": [
    "ui",
    "react"
  ],
  "author": "Jan Graichen <jg@altimos.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgraichen/molecule/issues"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "microtime": "^2.1.3",
    "tether": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "browserify": "^11.2.0",
    "coffee-script": "~1.8.0",
    "coffeeify": "^1.0.0",
    "envify": "^3.2.0",
    "exorcist": "^0.4.0",
    "jest-cli": "^0.5.10",
    "postcss-cli": "^2.2.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "sifter": "^0.4.4"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/lib"
    ]
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "envify"
    ]
  }
}
