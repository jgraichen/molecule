{
  "name": "molecule",
  "version": "0.1.0",
  "description": "Reactive UI molecules to build higher level components",
  "main": "lib/index.coffee",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jgraichen/molecule.git"
  },
  "keywords": [
    "ui",
    "react"
  ],
  "author": "Jan Graichen <jg@altimos.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgraichen/molecule/issues"
  },
  "dependencies": {
    "lodash": "^3.9.3",
    "tether": "^0.6.5"
  },
  "peerDependencies": {
    "react": "^0.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.1.1",
    "browserify": "~8.0.2",
    "coffee-script": "~1.8.0",
    "exorcist": "^0.1.6",
    "jest-cli": "^0.2.1",
    "sifter": "^0.3.4",
    "coffeeify": "~1.0.0",
    "envify": "~3.2.0",
    "react": "^0.13.0"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "testFileExtensions": [
      "coffee",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/lib"
    ]
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "envify"
    ]
  }
}
